{
    "family": "lexrag-frontend",
    "executionRoleArn": "arn:aws:iam::110266626277:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "<IMAGE_URI>",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "protocol": "tcp"
          }
        ],
        "environment": [
          {
            "name": "NEXT_PUBLIC_API_BASE_URL",
            "value": "https://api.lexrag.com"
          },
          {
            "name": "NEXT_PUBLIC_VERIFICATION_CODE_TTL",
            "value": "30"
          }
        ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }